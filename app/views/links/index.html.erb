<h1>Listing links</h1>

<table>
  <tr>
    <th>Url</th>
    <th>Count</th>
  </tr>

<% for link in @links %>
  <tr>
    <td><%=h link.id %></td>
    <td><%=h link.to_diminutive_url %></td>
    <td><%=h link.url %></td>
    <td><%=h link.count %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New link', new_link_path %>
